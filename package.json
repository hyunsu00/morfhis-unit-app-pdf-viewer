{
  "name": "pdf-viewer",
  "version": "0.0.1",
  "private": false,
  "dependencies": {
    "@emotion/cache": "^11.10.3",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/icons-material": "^5.10.3",
    "@mui/material": "^5.10.3",
    "@tanstack/react-query": "^4.2.3",
    "axios": "^0.21.1",
    "lodash": "^4.17.21",
    "react": "^17.0.1",
    "react-device-detect": "^2.2.2",
    "react-dom": "^17.0.1",
    "react-intl": "^5.13.4",
    "react-router-dom": "^5.2.0",
    "zustand": "^4.1.1"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "clean-dist": "rm -rf dist",
    "clean-package": "rm -rf package",
    "clean-package-tgz": "rm -rf package-tgz",
    "mkdir-package": "mkdirp package",
    "mkdir-package-tgz": "mkdirp package-tgz",
    "cp-package": "cp -r package.json README.md dist/Components/PdfViewer ./package",
    "tar-package-tgz": "tar cvzf package-tgz/pdf-viewer-0.0.1.tgz package",
    "package": "npm run build && npm run clean-dist && npm run clean-package && npm run clean-package-tgz && npm run mkdir-package && npm run mkdir-package-tgz && tsc && npm run cp-package && npm run tar-package-tgz && npm run clean-dist && npm run clean-package",
    "lib" : "npm run lib:pdf.js && npm run lib:pdf-annotate.js && npm run lib:annotpdf && npm run lib:pdf-lib",
    "clean" : "npm run clean:pdf.js && npm run clean:pdf-annotate.js && npm run clean:annotpdf && npm run clean:pdf-lib",
    "lib:pdf.js": "gulp -f ./extern/pdf.js/gulpfile.js generic && cpx \"./extern/pdf.js/build/generic/**/*\" ./public/libs/pdfjs",
    "clean:pdf.js": "gulp -f ./extern/pdf.js/gulpfile.js clean && rimraf ./public/libs/pdfjs",
    "lib:pdf-annotate.js": "npm --prefix ./extern/pdf-annotate.js run build && cpx \"./extern/pdf-annotate.js/dist/**/*\" ./public/libs",
    "clean:pdf-annotate.js": "npm --prefix ./extern/pdf-annotate.js run clean && rimraf ./public/libs/pdf-annotate.js ./public/libs/pdf-annotate.js.map",
    "lib:annotpdf": "npm --prefix ./extern/pdfAnnotate run build && cpx \"./extern/pdfAnnotate/_bundles/**/*\" ./public/libs",
    "clean:annotpdf": "npm --prefix ./extern/pdfAnnotate run clean && rimraf ./public/libs/annotpdf.js",
    "lib:pdf-lib": "npm --prefix ./extern/pdf-lib run build:es && npm --prefix ./extern/pdf-lib run build:umd && cpx \"./extern/pdf-lib/dist/**/*\" ./public/libs",
    "clean:pdf-lib": "npm --prefix ./extern/pdf-lib run clean && rimraf ./public/libs/pdf-lib.js ./public/libs/pdf-lib.js.map"
  },
  "homepage": "/hdv",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^16.11.21",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "cpx": "^1.5.0",
    "eslint": "^8.32.0",
    "html-loader": "^4.2.0",
    "http-proxy-middleware": "^1.3.1",
    "mkdirp": "^2.1.3",
    "prettier": "2.7.1",
    "react-app-rewired": "^2.2.1",
    "react-scripts": "5.0.0",
    "rimraf": "^4.1.2",
    "typescript": "^4.5.5",
    "web-vitals": "^2.1.4"
  }
}
